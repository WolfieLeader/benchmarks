{
  "name": "server-express",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write ./src",
    "lint": "biome lint ./src",
    "lint:fix": "biome check --fix ./src"
  },
  "dependencies": {
    "cassandra-driver": "^4.8.0",
    "cookie-parser": "^1.4.7",
    "drizzle-orm": "^0.45.1",
    "express": "^5.2.1",
    "ioredis": "^5.9.2",
    "mongodb": "^7.0.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pg": "^8.18.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.2.0",
    "@types/pg": "^8.16.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
