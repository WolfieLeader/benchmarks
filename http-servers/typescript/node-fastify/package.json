{
  "name": "fastify-server",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write ./src",
    "lint": "biome lint ./src",
    "lint:fix": "biome check --fix ./src"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/formbody": "^8.0.2",
    "@fastify/multipart": "^9.4.0",
    "cassandra-driver": "^4.8.0",
    "drizzle-orm": "^0.45.1",
    "fastify": "^5.7.2",
    "ioredis": "^5.9.2",
    "mongodb": "^7.0.0",
    "pg": "^8.18.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/node": "^25.1.0",
    "@types/pg": "^8.16.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
