        {
          "datasource": { "type": "influxdb", "uid": "${DS_INFLUXDB}" },
          "rawSql": "WITH server_total AS (SELECT server, AVG(total_duration_ns) AS avg_latency FROM sequence_latency WHERE time >= now() - INTERVAL '24 hours' AND run_id = '${run_id}' AND sequence_id = 'crud' GROUP BY server) SELECT s.server, s.database, AVG(s.total_duration_ns) AS latency FROM sequence_latency s JOIN server_total t ON s.server = t.server WHERE s.time >= now() - INTERVAL '24 hours' AND s.run_id = '${run_id}' AND s.sequence_id = 'crud' GROUP BY s.server, s.database, t.avg_latency ORDER BY t.avg_latency, s.database",
