          "datasource": { "type": "influxdb", "uid": "${DS_INFLUXDB}" },
          "rawSql": "WITH stats AS (SELECT server, SUM(CAST(p50_ns AS DOUBLE) * count) / SUM(count) AS p50, SUM(CAST(p95_ns AS DOUBLE) * count) / SUM(count) AS p95 FROM endpoint_stats WHERE time >= now() - INTERVAL '24 hours' AND run_id = '${run_id}' AND ('${endpoint}' = '' OR '${endpoint}' = 'overall' OR endpoint = '${endpoint}') AND ('${source}' = '' OR '${source}' = 'all' OR source = '${source}') GROUP BY server), ranked AS (SELECT server, p50, p95 - p50 AS delta, CAST(ROW_NUMBER() OVER (ORDER BY p50) AS BIGINT) AS rank FROM stats) SELECT server AS name, CASE WHEN t.type = 'p50' THEN server ELSE CONCAT(server, '_delta') END AS series, CASE WHEN t.type = 'p50' THEN p50 ELSE delta END AS value FROM ranked CROSS JOIN (SELECT 'p50' AS type UNION ALL SELECT 'delta') AS t ORDER BY rank, series",
